                    label.reg-form__check-block(for="chekInput15")
                      input.check-input#chekInput15(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Speciality
                  .reg-form__col
                    label.reg-form__check-block(for="chekInput16")
                      input.check-input#chekInput16(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Tugboat
