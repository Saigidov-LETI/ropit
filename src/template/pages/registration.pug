                    label.reg-form__check-block(for="chekInput7")
                      input.check-input#chekInput7(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Refer
                    label.reg-form__check-block(for="chekInput8")
                      input.check-input#chekInput8(type="checkbox")
                      .input-my-checkbox
