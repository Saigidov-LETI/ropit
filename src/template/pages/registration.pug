extends ../loyout

block variables
    - var title= "Регистрация"

block content
  section.reg-section
    .container
      h2.section-title Registration
      .reg
        ul.reg-list
          li.reg-item.reg-item--user.reg-item--error.active
            a.reg-item__link User info
          li.reg-item.reg-item--company.reg-item--error.active
            a.reg-item__link Company info
        .form-content
          .form-block.form-block--user.active 
            .error-message You entered incorrect data or did not enter the necessary data
            .reg-form__top.reg-form__top--flex
              .reg-form__col
                label.reg-form__block(for="")
                  .reg-form__name Title #[span.red-star *]
                  select.form-user__select(name="userTitle" value="")
                      //- option.form-user__option(selected) 
                      option.form-user__option MR
                      option.form-user__option MS
                      option.form-user__option MISS
                      option.form-user__option MRS
                      option.form-user__option DR
                      option.form-user__option CPTN
                      option.form-user__option PROF
                label.reg-form__block(for="")
                  .reg-form__name First Name #[span.red-star *]
                  input.reg-form__input.reg-form__input--error(type="text")
                label.reg-form__block(for="")
                  .reg-form__name Last Name #[span.red-star *]
                  input.reg-form__input(type="text")
              .reg-form__col
                label.reg-form__block(for="")
                  .reg-form__name E-mail #[span.red-star *]
                  input.reg-form__input(type="text")
                label.reg-form__block(for="")
                  .reg-form__name Password #[span.red-star *]
                  .reg-form__input-block
                    input.reg-form__input(type="text")
                    .reg-form__prompt **Your password must be at least 6 characters and should ideally include both letters and numbers
                label.reg-form__block(for="")
                  .reg-form__name Confirm Password #[span.red-star *]
                  .reg-form__input-block
                    input.reg-form__input(type="text")
                    .reg-form__prompt **Please confirm your password by entering it again
              .reg-form__col
                label.reg-form__block(for="")
                  .reg-form__name Type of profile #[span.red-star *]
                  select.form-user__select(name="typeProfile" value="")
                    //- option.form-user__option(selected) 
                    option.form-user__option Profile 1
                    option.form-user__option Profile 2
                    option.form-user__option Profile 3
                    option.form-user__option Profile 4
            .reg-form__bottom
                .reg-bottom__left
                  .reg-bottom__text * In future you will be asked for your corporate e-mail adress and password to login this website
                  label.reg-form__check-block(for="chekCompany")
                      input.check-input#chekCompany(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check My company already haved account on this website
                .reg-bottom__right
                  button.btn.reg-bottom__next(type="button") Next Step
          .form-block.form-block--company
            .error-message You entered incorrect data or did not enter the necessary data
            form.reg-form
              .reg-form__top
                .reg-form__row
                  label.reg-form__block(for="")
                    .reg-form__name Company name #[span.red-star *]
                    input.reg-form__input(type="text")
                  label.reg-form__block(for="")
                    .reg-form__name Adress 1 #[span.red-star *]
                    input.reg-form__input(type="text")
                  label.reg-form__block(for="")
                    .reg-form__name Phone number #[span.red-star *]
                    input.reg-form__input(type="text")
                .reg-form__row
                  label.reg-form__block(for="")
                    .reg-form__name Country #[span.red-star *]
                    input.reg-form__input(type="text")
                  label.reg-form__block(for="")
                    .reg-form__name Adress 2 #[span.red-star *]
                    input.reg-form__input(type="text")
                  label.reg-form__block(for="")
                    .reg-form__name Web site #[span.red-star *]
                    input.reg-form__input(type="text")
                .reg-form__row
                  label.reg-form__block(for="")
                    .reg-form__name City #[span.red-star *]
                    input.reg-form__input(type="text")
                  label.reg-form__block(for="")
                    .reg-form__name ZIP / Post code #[span.red-star *]
                    input.reg-form__input(type="text")
                  label.reg-form__block(for="")
                    .reg-form__name Number of vessels #[span.red-star *]
                    input.reg-form__input(type="text")
              .reg-form__center
                .reg-form__title Choose types of your vessels
                .reg-form__check
                  .reg-form__col
                    label.reg-form__check-block(for="chekInput1")
                      input.check-input#chekInput1(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Bardes
                    label.reg-form__check-block(for="chekInput2")
                      input.check-input#chekInput2(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Chemical Carriers
