                    label.reg-form__check-block(for="chekInput10")
                      input.check-input#chekInput10(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Cruise
                  .reg-form__col
                    label.reg-form__check-block(for="chekInput11")
                      input.check-input#chekInput11(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Ferries
                    label.reg-form__check-block(for="chekInput12")
                      input.check-input#chekInput12(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check Hospital Ship
                    label.reg-form__check-block(for="chekInput13")
                      input.check-input#chekInput13(type="checkbox")
                      .input-my-checkbox
                      .reg-form__name.reg-form__name--check OSV
                    label.reg-form__check-block(for="chekInput14")
